<Project Sdk="Microsoft.NET.Sdk">
	<PropertyGroup>
		<AppendTargetFrameworkToOutputPath>False</AppendTargetFrameworkToOutputPath>
		<AppendRuntimeIdentifierToOutputPath>False</AppendRuntimeIdentifierToOutputPath>
		<!--PublishDirName>$(PublishTarget)</PublishDirName-->
		<!--IsPublish>False</IsPublish-->
		<BaseOutputPath>../bin/$(Configuration)</BaseOutputPath>
	</PropertyGroup>
	<PropertyGroup Condition=" '$(PublishTarget)' == 'Windows' ">
		<RuntimeIdentifier>win7-x64</RuntimeIdentifier>
		<!--IsPublish>True</IsPublish-->
	</PropertyGroup>
	<PropertyGroup Condition=" '$(PublishTarget)' == 'Linux' ">
		<RuntimeIdentifier>linux-x64</RuntimeIdentifier>
		<!--IsPublish>True</IsPublish-->
	</PropertyGroup>


	<!--PropertyGroup Condition=" '$(PublishTarget)' != '' ">
