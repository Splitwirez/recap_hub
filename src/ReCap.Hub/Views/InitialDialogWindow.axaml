<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:vm="using:ReCap.Hub.ViewModels"
        xmlns:v="using:ReCap.Hub.Views"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
        Icon="avares://ReCap.Hub/Res/Icon/ReCap.ico"
        x:Class="ReCap.Hub.Views.InitialDialogWindow"
        Title="Resurrection Capsule Hub"
        Width="200" Height="100"
        CanResize="False">
    <Design.DataContext>
        <vm:MainWindowViewModel/>
    </Design.DataContext>
  <ContentControl Content="{Binding CurrentDialog}"
                IsTabStop="False"
                Focusable="False"/>
</Window>