<ResourceDictionary xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:local="using:ReCap.CommonUI"
        xmlns:rctr="using:ReCap.CommonUI"
        xmlns:sys="using:System">
	<SolidColorBrush x:Key="WindowBodyBorderBrush">#FFC0C0C0</SolidColorBrush>

	<Color x:Key="WindowBodyBackgroundColor">#F7111213</Color>
	<Color x:Key="WindowBodyBackgroundGlowColor">#7FFFFFFF</Color>
	<Color x:Key="WindowBodyBackgroundGlowMidColor">#40FFFFFF</Color>
	<Color x:Key="WindowBodyBackgroundGlowEndColor">#00FFFFFF</Color>
	<SolidColorBrush x:Key="WindowBodyBackgroundBrush" Color="{DynamicResource WindowBodyBackgroundColor}"/>
	<GradientStops x:Key="WindowBodyBackgroundGlowStops">
		<GradientStop Offset="0" Color="{DynamicResource WindowBodyBackgroundGlowColor}"/>
		<GradientStop Offset="0.375" Color="{DynamicResource WindowBodyBackgroundGlowMidColor}"/>
		<GradientStop Offset="1" Color="{DynamicResource WindowBodyBackgroundGlowEndColor}"/>
	</GradientStops>



	<LinearGradientBrush x:Key="Window.TitleBar.BackgroundBrush.Active"
						StartPoint="0%,0%"
						EndPoint="0%,100%">
		<GradientStop Offset="0" Color="#BBB7CFDA"/>
		<GradientStop Offset="1" Color="#7C93A5AD"/>
		<!--GradientStop Offset="0.38" Color="#72BBD2DC"/-->
	</LinearGradientBrush>
	<LinearGradientBrush x:Key="Window.TitleBar.BackgroundBrush.Inactive"
						StartPoint="0%,0%"
						EndPoint="0%,100%">
		<GradientStop Offset="0" Color="#BBD8D8D8"/>
		<GradientStop Offset="1" Color="#7CAAAAAA"/>
		<!--GradientStop Offset="0.38" Color="#72DBDBDB"/-->
	</LinearGradientBrush>
	<Color x:Key="Window.Chrome.BorderBrush.Color0">#40FFFFFF</Color>
	<Color x:Key="Window.Chrome.BorderBrush.Color1">#BDA4B2B9</Color>
	<Color x:Key="Window.Chrome.BorderBrush.Color2">#C0FFFFFF</Color>
	<GradientStops x:Key="Window.Chrome.BorderBrush.GradientStops">
		<GradientStop Offset="0" Color="{DynamicResource Window.Chrome.BorderBrush.Color0}" />
		<GradientStop Offset="1" Color="{DynamicResource Window.Chrome.BorderBrush.Color2}" />
		<!--GradientStop Offset="0" Color=""/>
		<GradientStop Offset="0.5" Color="#E0FFFFFF"/>
		<GradientStop Offset="0.5" Color="#EDA4B2B9"/>
		<GradientStop Offset="1" Color="#E0FFFFFF"/-->
	</GradientStops>
	<LinearGradientBrush x:Key="Window.TitleBar.BorderBrush"
						StartPoint="0,0"
						EndPoint="0,40"
						GradientStops="{DynamicResource Window.Chrome.BorderBrush.GradientStops}" />
	<!--
						StartPoint="0%,0%"
						EndPoint="0%,100%"-->
	<!--
		<GradientStop Offset="0" Color="{DynamicResource Window.Chrome.BorderBrush.Color0}" />
		<GradientStop Offset="1" Color="{DynamicResource Window.Chrome.BorderBrush.Color2}" />
	</LinearGradientBrush-->
