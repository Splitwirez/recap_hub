	<SolidColorBrush x:Key="Window.HoloShadow.Background.Translucent">#9D0D0F12</SolidColorBrush>
	<SolidColorBrush x:Key="Window.HoloShadow.BorderBrush.Translucent">#6F5D5F62</SolidColorBrush>
	
	<SolidColorBrush x:Key="Window.HoloShadow.Background.Opaque.Active">#FF25272A</SolidColorBrush>
	<SolidColorBrush x:Key="Window.HoloShadow.Background.Opaque.Inactive">#FF3D3F42</SolidColorBrush>
	<SolidColorBrush x:Key="Window.HoloShadow.BorderBrush.Opaque">#FF515356</SolidColorBrush>
