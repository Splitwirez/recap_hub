	<LinearGradientBrush x:Key="Window.Body.Background"
						StartPoint="0,0"
						EndPoint="0,4"
						SpreadMethod="Repeat">
		<GradientStop Offset="0.5" Color="#C0030303"/>
		<GradientStop Offset="0.75" Color="#C0070707"/>
	</LinearGradientBrush>
	<!--e0000000-->
	<LinearGradientBrush x:Key="Window.Body.BorderBrush"
						StartPoint="0%,-50%"
						EndPoint="0%,150%">
		<GradientStop Offset="-0.25" Color="{DynamicResource Window.Chrome.BorderBrush.Color2}" />
		<GradientStop Offset="0.5" Color="{DynamicResource Window.Chrome.BorderBrush.Color1}" />
		<GradientStop Offset="1.25" Color="{DynamicResource Window.Chrome.BorderBrush.Color2}" />
	</LinearGradientBrush>
	<!--
						StartPoint="0%,-200%"
						EndPoint="0%,200%"
						GradientStops="{DynamicResource Window.Chrome.BorderBrush.GradientStops}"/-->
	<!--SolidColorBrush x:Key="Window.Body.BorderBrush">#EDA4B2B9</SolidColorBrush-->

	<LinearGradientBrush x:Key="Window.BottomEdgeTrim.BorderBrush"
						StartPoint="0%,-25%"
						EndPoint="0%,100%"
						GradientStops="{DynamicResource Window.Chrome.BorderBrush.GradientStops}" />
