		<GradientStop Offset="0.125" Color="#FF964B4B" />
		<GradientStop Offset="1" Color="#FF562B2B" />
	</RadialGradientBrush>
	<Color x:Key="CaptionButton.BorderBrush.Idle.Inactive">#FFDDD4D4</Color>
	<Color x:Key="CaptionButton.Foreground.Idle.Inactive">#FFDD6E6E</Color>
