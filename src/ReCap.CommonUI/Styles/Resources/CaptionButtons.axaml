<ResourceDictionary xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
	<RadialGradientBrush x:Key="CaptionButton.Background.Idle"
						Center="50%,50%"
						GradientOrigin="50%,50%"
						 Radius="0.625">
		<GradientStop Offset="0.125" Color="#FF970000" />
		<GradientStop Offset="1" Color="#FF590000" />
	</RadialGradientBrush>
	<Color x:Key="CaptionButton.BorderBrush.Idle">#FFDECCCC</Color>
	<Color x:Key="CaptionButton.Foreground.Idle">#FFDE0000</Color>


	<RadialGradientBrush x:Key="CaptionButton.Background.Idle.Inactive"
						Center="50%,50%"
						GradientOrigin="50%,50%"
						 Radius="0.625">
		<GradientStop Offset="0.125" Color="#FF6C4D4D" />
		<GradientStop Offset="1" Color="#FF554343" />
	</RadialGradientBrush>
	<Color x:Key="CaptionButton.BorderBrush.Idle.Inactive">#FFDDD9D9</Color>
	<Color x:Key="CaptionButton.Foreground.Idle.Inactive">#FF875A5A</Color>
