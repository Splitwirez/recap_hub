	<RadialGradientBrush x:Key="CaptionButton.Background.Hover"
						Center="50%,50%"
						GradientOrigin="50%,50%"
						 Radius="0.625">
		<GradientStop Offset="0.125" Color="#FFFF0000" />
		<GradientStop Offset="1" Color="#FFFD0E0E" />
	</RadialGradientBrush>
	<Color x:Key="CaptionButton.BorderBrush.Hover">#FFFFCCCC</Color>
	<Color x:Key="CaptionButton.Foreground.Hover">#FFFFFFFF</Color>


	<RadialGradientBrush x:Key="CaptionButton.Background.Pressed"
						Center="50%,50%"
						GradientOrigin="50%,50%"
						 Radius="0.625">
		<GradientStop Offset="0.125" Color="#FF940000" />
		<GradientStop Offset="1" Color="#FF560000" />
	</RadialGradientBrush>
	<Color x:Key="CaptionButton.BorderBrush.Pressed">#FFA00000</Color>
	<Color x:Key="CaptionButton.Foreground.Pressed">#FF120000</Color>


	<Color x:Key="CaptionButton.Background.Disabled">#FF2D2D2D</Color>
	<Color x:Key="CaptionButton.BorderBrush.Disabled">#FFD1D1D1</Color>
	<Color x:Key="CaptionButton.Foreground.Disabled">#FF898989</Color>
