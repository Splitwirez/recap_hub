						Orientation="Horizontal"
						ZIndex="99999999">
					<Button x:Name="PART_FullScreenButton" Classes="captionButton"
							IsVisible="False">
					</Button>
					<Button x:Name="PART_MinimiseButton" Classes="captionButton">
						<Path Data="M 0 0 L 13 0 L 13 4 L 0 4 Z"/>
